<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="course">
  	<select id="courseList" parameterType="list" resultType="courseDto">
  		SELECT C_NO, C_NAME, ENT_PERSONNEL, C_STATUS, C_REGDATE, C_COUNT
  		FROM mydb.COURSE
  		ORDER BY C_NO DESC
  	</select>
  
  </mapper>